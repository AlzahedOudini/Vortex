<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="wrap_content">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blanc"
    tools:context=".ProfileAndSetting.Setting">


    <ImageView
        android:id="@+id/backspace_blanc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/backspace_black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logoblanc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/logoblanc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.951"
        app:layout_constraintStart_toEndOf="@id/backspace_blanc"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/profile_pic"
        android:layout_width="159dp"
        android:layout_height="137dp"
        android:layout_marginTop="16dp"
        app:civ_shadow="true"
        app:civ_border_width="1dp"
        android:foregroundGravity="center"
        android:src="@drawable/photo_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/logoblanc" />

    <TextView
        android:id="@+id/user_name_profile"
        style="@style/user_name_profile"
        android:layout_width="263dp"
        android:layout_height="46dp"
        android:layout_marginTop="8dp"
        android:drawableRight="@drawable/angleright"
        android:gravity="center"
        android:text="@string/user_name_profile"
        app:layout_constraintEnd_toEndOf="@id/profile_pic"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="@id/profile_pic"
        app:layout_constraintTop_toBottomOf="@id/profile_pic"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="158dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/back_ground_btn_sign_in_login"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="@id/user_name_profile"
        app:layout_constraintStart_toStartOf="@id/user_name_profile"
        app:layout_constraintTop_toBottomOf="@id/user_name_profile"
        tools:ignore="MissingConstraints">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_medium"
            android:text="Gold"
            android:textColor="#F9F222"
            android:textSize="12sp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5sp"
            android:fontFamily="@font/poppins_medium"
            android:text="experience"
            android:textColor="#fff"
            android:textSize="12sp"
            tools:ignore="MissingConstraints" />
    </LinearLayout>


    <ImageView
        android:id="@+id/divider"
        android:layout_width="400dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/ligne"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linearLayout2"
        app:layout_constraintVertical_bias="0.009"
        tools:ignore="MissingConstraints"/>


    <TextView
        android:id="@+id/setting_item_terms"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/Term"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_terms"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_coupon"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <ImageView
        android:id="@+id/iv_setting_privacy"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_private_policy"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_privacy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_terms"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_setting_aboutus"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_about_us"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_helpcenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_privacy"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_setting_helpcenter"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="32dp"
        android:src="@drawable/icon_help"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_aboutus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_aboutus"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/setting_item_helpcenter"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/help"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_aboutus"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_privacy"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"
        />

    <TextView
        android:id="@+id/setting_item_privacy"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/Private"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_privacy"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_terms"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <TextView
        android:id="@+id/setting_item_aboutus"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="32dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/About"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_helpcenter"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_helpcenter"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <ImageView
        android:id="@+id/iv_setting_terms"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_term_condition"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_terms"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_coupon"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/setting_item_coupon"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/Coupons"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_coupon"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_language"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <ImageView
        android:id="@+id/iv_setting_coupon"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_coupons"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_coupon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_language"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/setting_item_language"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/Languages"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_language"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_wallets"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <ImageView
        android:id="@+id/iv_setting_language"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_language"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_language"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_wallets"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/setting_item_wallets"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/Wallets"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_wallets"
        app:layout_constraintTop_toBottomOf="@+id/setting_item_notification"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <ImageView
        android:id="@+id/iv_setting_wallets"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_wallets"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_wallets"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_setting_notification"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/setting_item_notification"
        style="@style/setting_items"
        android:layout_width="240dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="48dp"
        android:drawableRight="@drawable/ic_chevron_right_black_24dp"
        android:text="@string/Notification"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_setting_notification"
        app:layout_constraintTop_toBottomOf="@+id/divider"
        tools:ignore="MissingConstraints"
        android:onClick="showPopup"/>

    <ImageView
        android:id="@+id/iv_setting_notification"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/icon_notification"
        app:layout_constraintEnd_toStartOf="@+id/setting_item_notification"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>